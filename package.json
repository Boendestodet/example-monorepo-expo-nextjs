{
  "name": "avivox",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:mobile": "turbo run dev --filter=\"{./apps/mobile}...\"",
    "dev:web": "turbo run dev --filter=\"{./apps/web}...\"",
    "build": "turbo run build",
    "build:mobile": "turbo run build --filter=\"{./apps/mobile}...\"",
    "build:web": "turbo run build --filter=\"{./apps/web}...\"",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "dependencies": {},
  "resolutions": {
    "metro": "0.73.7",
    "metro-babel-transformer": "0.73.7",
    "metro-cache": "0.73.7",
    "metro-file-map": "0.73.7",
    "metro-hermes-compiler": "0.73.7",
    "metro-inspector-proxy": "0.73.7",
    "metro-react-native-babel-transformer": "0.73.7",
    "metro-resolver": "0.73.7",
    "metro-runtime": "0.73.7",
    "metro-source-map": "0.73.7",
    "metro-symbolicate": "0.73.7",
    "metro-transform-plugins": "0.73.7",
    "metro-transform-worker": "0.73.7",
    "metro-cache-key": "0.73.7",
    "metro-config": "0.73.7",
    "metro-core": "0.73.7",
    "metro-minify-terser": "0.73.7",
    "metro-minify-uglify": "0.73.7",
    "metro-react-native-babel-preset": "0.73.7"
  },
  "packageExtensions": {
    "eslint-config-next@*": {
      "dependencies": {
        "next": "*"
      }
    },
    "@next/font@*": {
      "dependencies": {
        "next": "*"
      }
    }
  }
}
