{
  "name": "mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=8192' expo start --go",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' expo export --output-dir ./build --platform all",
    "start": "NODE_OPTIONS='--max-old-space-size=8192' expo start --dev-client",
    "lint": "expo lint",
    "test": "echo \"mobile: no test specified\"",
    "clean": "rm -rf .expo .turbo build",
    "type-check": "tsc --noEmit",
    "release": "NODE_OPTIONS='--max-old-space-size=8192' eas build --profile development --platform android"
  },
  "dependencies": {
    "@gluestack-ui/nativewind-utils": "^1.0.25",
    "@monorepo/hello-ui": "workspace:*",
    "expo": "~52.0.11",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.4",
    "expo-linking": "~7.0.3",
    "expo-router": "^4.0.9",
    "expo-status-bar": "~2.0.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "3.16.3",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-svg": "15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@monorepo/eslint-config": "workspace:*",
    "@monorepo/tsconfig": "workspace:*",
    "@testing-library/react-native": "12.9.0",
    "@types/react": "~18.3.12",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "8.57.0",
    "tailwindcss": "^3.4.15",
    "typescript": "5.7.2"
  }
}
