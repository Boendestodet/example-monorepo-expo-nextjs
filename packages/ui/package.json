{
  "name": "ui",
  "version": "0.1.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "react-native": "./src/index.ts",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "dev": "tsup --watch --silent",
    "lint": "eslint --ext js,ts,tsx .",
    "test": "jest",
    "build": "tsup --clean"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^26.0.24",
    "@types/react": "~18.0.25",
    "@types/react-native": "~0.70.6",
    "babel-preset-expo": "~9.2.1",
    "jest": "^27.0.0",
    "jest-expo": "^47.0.1",
    "@testing-library/react-native": "^11.4.0",
    "react-test-renderer": "18.1.0",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "tsup": "^6.5.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-web": "*"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-web": {
      "optional": true
    }
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ]
  },
  "eslintConfig": {
    "extends": "eslint-config-custom",
    "ignorePatterns": [
      "node_modules",
      "build"
    ]
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "outDir": "build",
    "sourcemap": true,
    "dts": true,
    "external": [
      "react",
      "react-native",
      "react-native-web"
    ]
  }
}
