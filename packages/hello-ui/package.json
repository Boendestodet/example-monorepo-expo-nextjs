    "@testing-library/react-native": "^12.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "18.13.0",
    "@types/react": "^18.0.33",
    "@types/react-native": "~0.71.5",
    "@types/react-test-renderer": "^18.0.0",
    "babel-preset-expo": "^9.3.2",
    "eslint": "8.38.0",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "react": "^18.2.0",
    "react-native": "0.71.6",
    "react-native-svg": "13.4.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
