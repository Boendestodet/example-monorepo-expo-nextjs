  "main": "build/index.js",
  "types": "build/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "rm -rf build tsconfig.tsbuildinfo && babel \"src\" -d \"build\" --extensions \".ts,.tsx,.js,.jsx\" --ignore \"**/__tests__\" && tsc --emitDeclarationOnly",
